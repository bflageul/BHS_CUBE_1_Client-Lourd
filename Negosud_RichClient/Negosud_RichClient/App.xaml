<Application x:Class="Negosud_RichClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Negosud_RichClient"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- Themes -->
                <ResourceDictionary Source="Styles/ColorBrush.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/CoreResource.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/DataWindowResource.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/UserControlResource.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
